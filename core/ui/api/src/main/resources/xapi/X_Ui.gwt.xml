<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN" "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module>
  <inherits name="xapi.X_Inject"/>
  <inherits name="xapi.X_Event" />
  <source path="gwt/ui" />
  <source path="ui/api" />
  <source path="ui/api/event" />
  <source path="ui/service" />
  <source path="ui/impl" />

  <!-- This configuration property is used to automatically add a stable withPrefix to html data attribute names -->
  <define-configuration-property name="data.attr.prefix" is-multi-valued="false" />
  <set-configuration-property name="data.attr.prefix" value="xapi-" />

</module>
