<xapi-schema
    inherit = false
    platforms = [ main ]
    modules = [
        <main
            requires = {
                project: ":xapi-collect",
                @transitive
                project: { ":log" : api },
                project: ":xapi-core-parent:xapi-core-reflect",

                project: { ":base" : api }
            }
        /main>
    ]
/xapi-schema>